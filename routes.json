      "verbs": ["get"],
      "path": "/:slide",
      "regex": {"slide": "[0-9]*"},
      "call": "preso.index"
    },
    "weather": {
      "verbs": ["get"],
      "path": "/weather",
      "call": "weather.index"
    },
    "settings": [ "master" ],
    "conf": {
      "verbs": ["get"],
      "path": "/conf",
      "call": "conf.index"
    }
}]
